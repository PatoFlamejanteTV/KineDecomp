<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="rectangle">
            <size
                android:height="18.5dp"
                android:width="18.5dp"/>
            <corners android:radius="3dp"/>
            <gradient
                android:startColor="#fffafafa"
                android:endColor="#ffe2e2e2"
                android:angle="-90"/>
            <stroke
                android:width="1dp"
                android:color="#ff000000"/>
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp"/>
        </shape>
    </item>
    <item>
        <shape
            android:dither="true"
            android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#ffefefef"/>
            <corners android:radius="2dp"/>
            <padding
                android:left="4dp"
                android:top="4dp"
                android:right="4dp"
                android:bottom="4dp"/>
        </shape>
    </item>
</layer-list>
