<?xml version="1.0" encoding="utf-8"?>
<com.adobe.creativesdk.foundation.internal.storage.controllers.AdobeStorageListViewAssetViewCell xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/adobe_csdk_storage_assetlist_assetcell_root"
    android:descendantFocusability="blocksDescendants"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_gravity="center"
        android:orientation="vertical"
        android:descendantFocusability="blocksDescendants"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/adobe_storage_assetslist_header"/>
        <include layout="@layout/adobe_storage_assetslist_divider"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_gravity="center_vertical"
            android:orientation="horizontal"
            android:descendantFocusability="blocksDescendants"
            android:layout_width="match_parent"
            android:layout_height="@dimen/adobe_csdk_assetbrowser_list_view_item_height">
            <ImageButton
                android:id="@+id/adobe_csdk_storage_assetlist_assetcell_selectbtn"
                android:background="@null"
                android:visibility="gone"
                android:layout_width="@dimen/adobe_csdk_storage_assetslist_selector_btn_width"
                android:layout_height="@dimen/adobe_csdk_storage_assetslist_selector_btn_width"
                android:src="@drawable/storage_list_assetcell_select_btn_selector"
                android:scaleType="center"/>
            <ImageView
                android:id="@+id/adobe_csdk_storage_assetlist_assetcell_imageView"
                android:layout_width="@dimen/adobe_csdk_assetbrowser_list_view_image_dimension"
                android:layout_height="@dimen/adobe_csdk_assetbrowser_list_view_image_dimension"
                android:scaleType="center"/>
            <com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
                android:textSize="@dimen/adobe_csdk_assetbrowser_list_view_text_size"
                android:textColor="@color/adobe_csdk_asset_browser_dark_text"
                android:ellipsize="middle"
                android:gravity="center_vertical"
                android:id="@+id/adobe_csdk_storage_assetlist_assetcell_title"
                android:background="@color/adobe_csdk_transparent_color"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/adobe_csdk_assetbrowser_list_view_text_left_padding"
                android:layout_marginRight="@dimen/adobe_csdk_assetbrowser_list_view_text_right_padding"
                android:singleLine="true"/>
        </LinearLayout>
        <ProgressBar
            android:ellipsize="middle"
            android:id="@+id/adobe_csdk_assetlist_upload_progress_bar"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/adobe_csdk_assetbrowser_list_view_progress_bar_height"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:max="100"
            android:indeterminate="false"
            android:progressDrawable="@drawable/asset_upload_progress_bar"
            android:minHeight="@dimen/adobe_csdk_assetbrowser_list_view_progress_bar_height"
            android:singleLine="true"
            style="@android:style/Widget.ProgressBar.Horizontal"/>
    </LinearLayout>
</com.adobe.creativesdk.foundation.internal.storage.controllers.AdobeStorageListViewAssetViewCell>
