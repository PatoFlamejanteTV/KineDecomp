<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:paddingTop="6.3dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textSize="@dimen/subscription_title_min_text_size"
        android:textColor="@android:color/white"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/upgrade_to_professional_edition"/>
    <TextView
        android:textSize="@dimen/subscription_subtitle_min_text_size"
        android:textColor="@android:color/white"
        android:layout_gravity="center_horizontal"
        android:id="@+id/tv_subscribe_info_doinstall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/download_store_message"/>
</LinearLayout>
