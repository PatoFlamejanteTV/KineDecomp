<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="center"
    android:background="@android:color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">
        <ImageView
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/oneup_no_nopreview"/>
        <com.adobe.creativesdk.foundation.internal.utils.CreativeSDKBoldTextView
            android:textSize="@dimen/adobe_csdk_asset_viewer_no_preview_textSize"
            android:textColor="@color/adobe_csdk_asset_browser_dark_text"
            android:id="@+id/adobe_csdk_common_view_error_message"
            android:paddingTop="@dimen/adobe_csdk_asset_viewer_no_preview_padding"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/adobe_csdk_IDS_ASSET_VIEWER_NO_PREVIEW_AVAILABLE"
            android:layout_centerHorizontal="true"/>
    </LinearLayout>
</RelativeLayout>
