<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/adobe_csdk_asset_view_long_click_main_container"
    android:tag="LARGE"
    android:background="@drawable/adobe_asset_view_edit_dialog_shape"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/adobe_csdk_asset_edit_dialog_fragment_margin_left"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="8dp">
        <com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
            android:textSize="20dp"
            android:textColor="@color/adobe_csdk_asset_browser_light_text"
            android:id="@+id/adobe_csdk_asset_long_click_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/adobe_csdk_long_click_menu_container"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/adobe_csdk_asset_edit_dialog_fragment_margin_left"/>
</LinearLayout>
